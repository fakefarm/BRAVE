<div id="content_search">
  <h1>Discover <%= @users.first.role %> Members</h1>

  <center>
  <ul> 
  <% @users.each do |user| %>
    <li><%= link_to user.name.upcase, user_url(user.id) %>
      <img width="100" class="photo" alt="dummypic" src="assets/pic<%= rand(0..10)%>.png">
    <%= button_tag "ADD #{user.name.upcase}" %>
    </li>
  <% end %>
  </ul>
  </center>
</div><!-- .content -->