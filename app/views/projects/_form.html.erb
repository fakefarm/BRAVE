<%= form_for @project do |f| %>
<h3>NEW PROJECT</h3>
<% if @project.errors.any? %>
<div class="error_messages">
  <h2>Form is invalid</h2>
  <ul>
    <% @project.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>


<%= f.label :title %>
<%= f.text_field :title %>
<br/>
<br/>
<%= f.label :client_email %>
<%= f.collection_select :client_email, User.all, :email, :name %>
<br/>
<br/>
<%= f.label :project_amount %>
<%= f.text_field :project_amount %>
<br/>



<%= f.submit  %>
<% end %>